{
  "name": "kartunsdesktop",
  "version": "0.1.4",
  "private": true,
  "dependencies": {
    "@supercharge/filesystem": "^2.3.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@tinymce/tinymce-react": "^4.3.0",
    "buffer": "^6.0.3",
    "chart.js": "^4.4.0",
    "dompurify": "^3.0.6",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "exceljs": "^4.4.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-image-file-resizer": "^0.4.8",
    "react-router": "^6.16.0",
    "react-router-dom": "^6.16.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "universal-cookie": "^6.1.1",
    "web-vitals": "^2.1.4"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "author": "Febri Fahmi Hakim",
  "description": "KartUNS Desktop App",
  "scripts": {
    "start": "electron-forge start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"cross-env BROWSER=none react-scripts start\" \"npm run wait\" \"npm run electron --trace-warnings\" \"npm run css\"",
    "wait": "wait-on http://localhost:3000",
    "electron": "electron .",
    "css": "npx tailwindcss -i ./src/input.css -o ./src/tailwind.css --watch",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "electron": "^26.3.0",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.3.3",
    "wait-on": "^7.0.1"
  },
  "browser": {
    "fs": false
  },
  "directories": {
    "buildResources": "build",
    "app": "build"
  },
  "files": [
    "**/*",
    "!**/*.ts",
    "!*.map",
    "!package.json",
    "!package-lock.json"
  ]
}
